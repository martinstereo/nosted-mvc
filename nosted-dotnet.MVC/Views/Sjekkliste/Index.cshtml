@model List<nosted_dotnet.MVC.Entities.Sjekkliste>

<h1>Sjekklisteoversikt:</h1>

<table class="table">
    <thead>
    <tr>
        <th>Sjekkliste ID</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var checklist in Model)
    {
        <tr>
            <td>@checklist.Id</td>
            <td>
                <a href="@Url.Action("GeneratePdf", "PdfGeneration", new { orderType = "Sjekkliste", orderId = checklist.Id })">Opprett PDF</a>
            </td>
        </tr>
    }
    </tbody>
</table>
